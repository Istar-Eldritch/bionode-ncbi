  "repository": {
    "type": "git",
    "url": "http://github.com/bionode/bionode-ncbi.git"
  },
  "author": {
    "name": "Bruno Vieira",
    "email": "mail@bmpvieira.com"
  },
  "dependencies": {
    "JSONStream": "^0.8.4",
    "async": "^0.9.0",
    "cheerio": "^0.17.0",
    "concat-stream": "~1.4.6",
    "debug": "^1.0.3",
    "dld": "git+https://github.com/bmpvieira/dld#0.0.2/pull-1",
    "docco": "^0.6.3",
    "minimist": "^1.1.0",
    "mkdirp": "^0.5.0",
